                                    <fo:block id="d649e3490"/>
                                    <fo:block id="d649e3493"/>
                                    <fo:block id="d649e3496"/>
                                    <fo:block id="d649e3499"/>
                                    <fo:block-container font-size="1.4em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid" id="ariaid-title35"><fo:block end-indent="0" start-indent="0" role="H3"><fo:inline>Navigator Physical View</fo:inline></fo:block></fo:block-container>
                                    
                                    <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">The default
                                            Navigator view is Physical and shows the hierarchy of
                                            your monitored enterprise. The Navigator Physical view
                                            is a discovered view: as new managed systems come online
                                            or when they become disconnected, the view is adjusted
                                            accordingly.</fo:block></fo:block-container>
