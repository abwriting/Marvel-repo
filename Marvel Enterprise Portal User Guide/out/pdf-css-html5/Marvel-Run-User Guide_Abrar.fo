                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Use <fo:inline chfox:inch="true" height="10px" max-width="100%" width="10px" id="unique_103_Connect_42_image_r54_b5t_zxb" chfox:nd-id="image_r54_b5t_zxb"><fo:inline><fo:external-graphic src="file:/C:/Users/AbrarSyed/Downloads/Marvel%20DITA%20project/June2023-DITAXML-Assignment-Abrar/Chapter4/Images/I_create_child_item.png" max-width="100%" scaling="non-uniform" content-width="10px" content-height="10px"/></fo:inline></fo:inline>
