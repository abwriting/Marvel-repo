                                    <fo:block role="Sect">
                                        <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="40px" space-after="1.33em" space-before="1.33em">
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">1. </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
                                                <fo:inline>Click <fo:inline chfox:inch="true" height="10px" max-width="100%" width="10px" id="unique_103_Connect_42_image_nh4_zxt_zxb" chfox:nd-id="image_nh4_zxt_zxb"><fo:inline><fo:external-graphic src="file:/C:/Users/AbrarSyed/Downloads/Marvel%20DITA%20project/June2023-DITAXML-Assignment-Abrar/Chapter4/Images/I_edit_navigator.png" max-width="100%" scaling="non-uniform" content-width="10px" content-height="10px"/></fo:inline></fo:inline>
                                                  <fo:inline font-weight="bold">Edit
                                                  Navigator View</fo:inline> in the Navigator toolbar if
                                                  the Navigator editor is not open.</fo:inline>
                                            </fo:block></fo:list-item-body></fo:list-item>
                                            <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">2. </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
                                                <fo:inline>Select the Navigator view
                                                  from the Target View <fo:inline chfox:inch="true" height="10px" max-width="100%" width="10px" id="unique_103_Connect_42_image_i4w_fyt_zxb" chfox:nd-id="image_i4w_fyt_zxb"><fo:inline><fo:external-graphic src="file:/C:/Users/AbrarSyed/Downloads/Marvel%20DITA%20project/June2023-DITAXML-Assignment-Abrar/Chapter4/Images/I_list.png" max-width="100%" scaling="non-uniform" content-width="10px" content-height="10px"/></fo:inline></fo:inline>
