                                    <fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Sect">
                                        <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">To edit the
                                            Navigator view follow the instructions:</fo:block></fo:block-container>
