                                                  <fo:inline chfox:inch="true" height="10px" max-width="100%" width="10px" id="unique_108_Connect_42_image_byl_v1n_zxb" chfox:nd-id="image_byl_v1n_zxb"><fo:inline><fo:external-graphic src="file:/C:/Users/AbrarSyed/Downloads/Marvel%20DITA%20project/June2023-DITAXML-Assignment-Abrar/Chapter4/Images/I_show_navigator_list.png" max-width="100%" scaling="non-uniform" content-width="10px" content-height="10px"/></fo:inline></fo:inline>
