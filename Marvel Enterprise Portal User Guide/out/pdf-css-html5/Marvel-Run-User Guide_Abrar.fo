                                    <fo:block-container font-size="1.4em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid" id="ariaid-title97"><fo:block end-indent="0" start-indent="0" role="H3"><fo:inline>Edit Workspace
                                        Properties</fo:inline></fo:block></fo:block-container>
                                    
                                    <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
