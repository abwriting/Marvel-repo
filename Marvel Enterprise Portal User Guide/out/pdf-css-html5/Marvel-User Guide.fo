                            <fo:block role="Sect" id="unique_32" chfox:nd-id="C_navigator_overview">
                                <fo:block id="d714e3735"/>
                                <fo:block-container font-size="1.7em" space-after="0.5em" margin-left="0" margin-right="0" space-before="1em" page-break-after="avoid" id="ariaid-title33"><fo:block end-indent="0" start-indent="0" role="H2"><fo:inline>Navigator Overview</fo:inline></fo:block></fo:block-container>
                                
                                <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                    <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0"/></fo:block-container>
                                    <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">The Navigator
                                        provides a hierarchical view of your enterprise. At the
                                        highest level you can get a high level overview of the
                                        status of your monitored environment. From there you can
                                        navigate to specific monitored resources to check activity
                                        and investigate problems.</fo:block></fo:block-container>
                                    <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">At every level,
                                        event indicators alert you to changes in system or
                                        application conditions.</fo:block></fo:block-container>
                                </fo:block></fo:block-container>
                            </fo:block>
                            <fo:block role="Sect" id="d681e289">
