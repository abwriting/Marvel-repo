                                                  <fo:inline chfox:inch="true" height="15px" max-width="100%" width="10px" id="unique_28_Connect_42_image_dgp_pb4_xxb" chfox:nd-id="image_dgp_pb4_xxb"><fo:inline><fo:external-graphic src="file:/C:/Users/AbrarSyed/Downloads/Marvel%20DITA%20project/Marvel-repo/Marvel%20Enterprise%20Portal%20User%20Guide/Chapter1/Images/I_action.png" max-width="100%" scaling="non-uniform" content-width="10px" content-height="15px"/></fo:inline></fo:inline>
